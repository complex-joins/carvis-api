{
  "name": "carvis-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:server": "babel ./src/server -d ./dist/server --presets es2015,stage-2,es2016 --copy-files",
    "build:tools": "babel ./tools -d ./_tools --presets es2015,stage-2,es2016 --copy-files",
    "build:db": "babel ./src/db -d ./dist/db --presets es2015,stage-2,es2016 --copy-files",
    "build:redis": "babel ./src/redis -d ./dist/redis --presets es2015,stage-2,es2016 --copy-files",
    "build": "npm run build:db; npm run build:server; npm run build:tools; npm run build:redis",
    "reset:db": "babel-node ./utils/wipeDB.js",
    "setup": "node ./tools/resolveGitIgnored.js;",
    "docker:deploy": "npm run build; docker build -t carvis/carvis-api .; docker push carvis/carvis-api",
    "deploy": "npm run docker:deploy; eb deploy",
    "migrate": "node _tools/migrate.js",
    "test": "redis-server & mocha --compilers js:babel-core/register ./tests/index.js",
    "start:dev": "npm run build:server -- -w & npm run build:db -- -w & npm run build:redis -- -w & npm run build:tools -- -w & nodemon -L ./dist/server/index.js",
    "start": "npm run build; node ./dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/complex-joins/carvis-api.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/complex-joins/carvis-api/issues"
  },
  "homepage": "https://github.com/complex-joins/carvis-api#readme",
  "dependencies": {
    "axios": "^0.13.1",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-regenerator": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-stage-2": "^6.13.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "btoa": "^1.1.2",
    "connect-redis": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.7.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "fakeredis": "^1.0.3",
    "jwt-simple": "^0.5.0",
    "lodash": "^4.15.0",
    "morgan": "^1.7.0",
    "node-fetch": "^1.6.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.0",
    "redis": "^2.6.2",
    "shelljs": "^0.7.3",
    "storkSQL": "^0.1.32",
    "twilio": "^2.9.2",
    "uuid-v4": "^0.1.0"
  },
